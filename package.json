{
  "name": "@xirelogy/rollup-plugin-xwts-i18n",
  "version": "1.0.3",
  "description": "Process xwts-i18n compatible locale translations with rollup",
  "keywords": [
    "rollup-plugin",
    "xwts",
    "xwts-i18n"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xirelogy/rollup-plugin-xwts-i18n"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rollup -c && tsc -P tsconfig.dts.json"
  },
  "files": [
    "dist",
    "!dist/**/*.map"
  ],
  "peerDependencies": {
    "rollup": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "rollup": {
      "optional": true
    }
  },
  "dependencies": {
    "picomatch": "^2.3.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/node": "^20",
    "rollup": "^3.0.0",
    "tslib": "^2.6.2",
    "typescript": "5.5"
  }
}
